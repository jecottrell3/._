#! /bin/sh

ESC=$(echo . | tr . \\033)

C=(K R G Y B M C W)
M=(D L [5]=L [25]=D)

for BG in 0 1 2 3 4 5 6 7
do
for BL in 25 5
do
for FG in 0 1 2 3 4 5 6 7
do
for HI in 0 1
do
    echo "$ESC[$HI;3${FG};4${BG};${BL}m${M[$HI]}${C[$FG]}${M[$BL]}${C[$BG]} " | tr -d \\012
#    echo "$ESC[$HI;5;3${FG};4${BG}m ${M[$HI]}${C[$FG]}${C[$BG]} " | tr -d \\012
done
done
echo ''
done
done
