set bell-style Visible
set output-meta On

"\M-\e": shell-expand-line
"\M-a": "\C-aman -a \n"
"\M-e": " --exclude="
"\M-h": "\C-e --help"
"\M-i": "META i"
"\M-j": "META j"
"\M-k": "\C-e | less +/^-.*\n"
"\M-m": "\C-e | less\n"
"\M-n": "\C-e | less -N\n"
"\M-o": overwrite-mode
"\M-q": 'PS1="$(hostname):\$PWD> "'
"\M-s": "\C-e | sort "
"\M-v": " --max-size="
"\M-w": "\C-w\C-y=$\C-y\e\C-e"
"\M-x": "fg; exit"
"\M-z": " --permanent"

"\M-%": "META % "
"\M-(": "META ( "
"\M-)": "META ) "
"\M-+": "META + "
"\M-,": "META , "
"\M-:": "; do echo =$x=; "
"\M-;": "; done\n"
"\M-@": "META @ "
"\M-]": "\C-aless \n"
"\M-\"": "META \" "
"\M-'": "META ' "

"\C-_": "\C-asudo "

"`": "\e"
"\M-`": self-insert
