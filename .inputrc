set bell-style Visible
set output-meta On

"\M-\e": shell-expand-line
"\M-a": "\C-aman -a \n"
"\M-e": " --exclude="
"\M-h": "for x in "
"\M-i": "; do echo =$x=; "
"\M-j": "; done\n"
"\M-k": "\C-e | less +/"
"\M-m": "\C-e | less\n"
"\M-o": overwrite-mode
"\M-q": 'PS1="$(hostname):\$PWD> "'
"\M-s": "\C-e | sort "
"\M-v": " --max-size="
"\M-w": "\C-w\C-y=$\C-y\e\C-e"
"\M-x": "fg; exit"
"\M-z": "lsb_release -sir\n"

"\M-%": "META % "
"\M-(": "META ( "
"\M-)": "META ) "
"\M-+": "META + "
"\M-,": "META , "
"\M-:": "META : "
"\M-;": "META ; "
"\M-@": "META @ "
"\M-]": "META ] "
"\M-\"": "META \" "
"\M-'": "META ' "

"\C-_": "\C-p\C-asudo "

"`": "\e"
"\M-`": self-insert
