#! /bin/sh

SSH=git@github.com:jecottrell3/._.git
WEB=https://github.com/jecottrell3/._.git
USR=https://jecottrell3@github.com/jecottrell3/._.git

case $1 in
(-s)	PULL=$SSH PUSH=$SSH;;
(-h)	PULL=$WEB PUSH=$WEB;;
(-p)	PULL=$WEB PUSH=$USR;;
(*)	echo "usage:	$0 -[shp]
	-s	SSH
	-h	HTTP
	-p	user@HTTP"; exit 1;;
esac

cd $RBJ
git remote -v
git remote -v set-url        origin $PULL
echo =====
git remote -v set-url --push origin $PUSH
git remote -v
