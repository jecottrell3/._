test -f .debug && echo .domain
# $Id$
#################################################################
#	Do Domain Specific Customization
#################################################################

  HOST=$(hostname -s)
#DOMAIN=$(hostname -d)
#  FQDN=$(hostname -f)
#    IP=$(hostname -i)

export H=$HOST D=$DOMAIN

case $FQDN in
(*.homerj)
#       For Cloning CDs
	alias	L='lsof /dev/scd0'
	alias	P='pkill kio_audio'
	alias	K='k3b'
	alias	S='pm-suspend'
	;;
(*.msc.fema.gov)	: zai
	alias	C='ssh -AX callisto'
	alias	V='ssh -AX VANGUARD'
	;;
(*)	: not special;;
esac

