test -f .debug && echo .domain
# $Id$
#################################################################
#	Do Domain Specific Customization
#################################################################

  HOST=$(hostname -s)
DOMAIN=$(hostname -d)
  FQDN=$(hostname -f)
    IP=$(hostname -i)

export H=$HOST D=$DOMAIN C=cit.nih.gov N=nih.gov

case $FQDN in
(*.homerj)
#       For Cloning CDs
	alias	L='lsof /dev/scd0'
	alias	P='pkill kio_audio'
	alias	K='k3b'
	alias	S='pm-suspend'
	;;
(*.nih.gov)
	export	BASE=ou=NIH,OU=AD,DC=NIH,DC=gov

	alias	lds="ldapsearch -b $BASE -h ldapad.nih.gov"
	alias	ldx="ldapsearch -b $BASE -h strudel.cit.nih.gov -x"

	export JRE6=java-1.6.0-openjdk
	export JDK6=java-1.6.0-openjdk-devel
	export JRE7=java-1.7.0-openjdk
	export JDK7=java-1.7.0-openjdk-devel

	export QR=/public/htdocs/rome.cit.nih.gov
	export QK=$QR/ks
	export QY=$QR/yum
	export QC=$QY/CentOS
	export QF=$QY/Fedora
	export QD=$QC/DCB
	export D5i=$QD/5/i386
	export D5x=$QD/5/x86_64
	export D6i=$QD/6/i386
	export D6x=$QD/6/x86_64
	export QB=$QC/base
	export B5i=$QB/5/os/i386
	export B5x=$QB/5/os/x86_64
	export B6i=$QB/6/os/i386
	export B6x=$QB/6/os/x86_64

	alias qaj='rpm -qa j\* | sort'

	for dir in /opt/openssh-dcb-5.?p1/bin
	do
		test -d $dir || continue
		case :$PATH: in
		(*:$dir:*)     continue;;
		(*)             PATH=$dir:$PATH;;
		esac
	done

	alias	pc='polka circulate'

	export  VP=/var/polka
	export          VPE=$VP/etc
	export          VPC=$VP/conf
	export  PRINTER=chessie
	export  LPDEST=$PRINTER
	;;
(*)	: not special;;
esac

#################################################################
