test -f $DEBUG && echo ..vars
#################################################################
#	REPO VARIABLES
#################################################################

UMD=mirror.umd.edu
KERN=mirrors.kernel.org
FEDORA=download.fedoraproject.org
EPEL5=http://$FEDORA/pub/epel/5/x86_64/epel-release-5-4.noarch.rpm
EPEL6=http://$FEDORA/pub/epel/6/x86_64/epel-release-6-8.noarch.rpm
EPEL7=http://$FEDORA/pub/epel/7/x86_64/epel-release-7-x.noarch.rpm

#################################################################
#	DIRECTORY and FILE VARIABLES
#################################################################

export	AM=/etc/auto.misc
export	CR=/etc/centos-release
export	FS=/etc/fstab
export	GR=/etc/group
export	HF=/etc/hosts
export	PW=/etc/passwd
export	SF=/etc/shadow
export	SU=/etc/sudoers

export	DH=/etc/dhcp
export		DH92=$DH/dhcpd.10.10.92
export		DH96=$DH/dhcpd.10.52.96
export		DHC=$DH/dhcpd.conf
export		DHH=$DH/dhcpd.host

export	F=fail2ban
export	FC=$F-client
export	FB=/etc/$F
export		FJ=$FB/jail.conf
export		FL=$FB/jail.local
export		FD=$FB/jail.d

export	GD=/etc/grub.d
export	GA=$GD/40_custom
export	GB=/boot/grub2
export	GC=$GB/grub.cfg

export	HA=/etc/ha.d
export		HACF=$HA/ha.cf
export		HARE=$HA/haresources

export	HT=/etc/httpd
export		HTC=$HT/conf
export		HTD=$HT/conf.d

export	IN=/etc/init.d
export	IT=/etc/inittab

export	KC=/etc/krb5.conf
export	KT=/etc/krb5.keytab

export	LDAP=/etc/openldap
export		LDCF=$LDAP/ldap.conf

export	LVM=/etc/lvm
export		LVC=$LVM/lvm.conf
export	MP=/etc/modprobe.d

export	NC=/etc/named.conf
export	ND=/var/named
export		SOA=/var/named/soa
export		PRI=/var/named/pri
export		SEC=/var/named/sec
export		MD=$PRI/md.ddn.com
export		N27=$SOA/10.10.27
export		N28=$SOA/10.10.28
export		N92=$SOA/10.10.92
export		N52=$SOA/10.52
export		N96=$N52.96
export		N112=$N52.112
export		N192=$SOA/192.168

export	NTP=/etc/ntp
export	NTC=$NTP.conf

export	PAM=/etc/pam.d
export	PRO=/etc/profile
export	RR=/etc/redhat-release
export	RC=/etc/resolv.conf

export	RD=/etc/reader.conf
export	RO=/etc/reader.conf.d/openct.conf

export	SSSD=/etc/sssd
export	SSSC=$SSSD/sssd.conf

export	SC=/etc/sysconfig
export		NET=$SC/network
export		NS=$SC/network-scripts
export			E0=$NS/ifcfg-eth0
export			E1=$NS/ifcfg-eth1
export			E2=$NS/ifcfg-eth2
export			E3=$NS/ifcfg-eth3
export			EM1=$NS/ifcfg-em1
export			EM2=$NS/ifcfg-em2
export			EM3=$NS/ifcfg-em3
export			EM4=$NS/ifcfg-em4

export	SM=/etc/mail
export		SMCF=$SM/sendmail.cf

export	SS=~/.ssh
export		AK=$SS/authorized_keys
export		KH=$SS/known_hosts
export		IDA=$SS/identity
export		IDP=$SS/identity.pub
export		DSA=$SS/id_dsa
export		DSP=$SS/id_dsa.pub
export		RSA=$SS/id_rsa
export		RSP=$SS/id_rsa.pub

export	PUP=$HOME/puppet
export		PN=$PUP/manifests
export		PM=$PUP/modules
export		PF=$PUP/files
export		PT=$PUP/templates

export	PK=/etc/pam_pkcs11
export		PKC=$PK/pam_pkcs11.conf

export	PKI=/etc/pki
export		PIV=$PKI/PIV
export		NSS=$PKI/nssdb

export	ES=/etc/security
export		ESA=$ES/access.conf

export	SSH=/etc/ssh
export		SSC=$SSH/ssh_config
export		SDC=$SSH/sshd_config
export		SKH=$SSH/sshd_known_hosts

export	UB=/usr/bin
export	UL=/usr/local
export		ULB=/usr/local/bin
export		ULS=/usr/local/sbin
export		ULO=/usr/local/opt
export		ULE=/usr/local/etc
export	US=/usr/share
export		USD=/usr/share/doc
export		USM=/usr/share/man
export	USB=/usr/sbin

export	X11=/etc/X11
export		XC=$X11/xorg.conf

export	CF=/lib/kbd/consolefonts
export	KF=/lib/kbd/consolefonts

export	VL=/var/log
export		VLB=$VL/boot.log
export		VLC=$VL/clamav
export		VLL=$VL/lastlog
export		VLM=$VL/messages
export		VLS=$VL/secure

export	WL=/var/lib
export		WLC=$WL/clamav
export		WLR=$WL/rpm
export		WLY=$WL/yum

export	YC=/etc/yum.conf
export	YR=/etc/yum.repos.d

#################################################################
