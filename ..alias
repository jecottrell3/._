defun	b() { mkdir -p "$@" && cd "$@"; }
defun	c() { cd    "$@" && ls -FAC; }
defun	u() { cd ../"$@" && ls -FAC; }
defun	s() { cd $(echo "$PWD" | sed "s%$1%$2%"); }
defun	i() { echo "$1=$PWD"; eval "$1=$PWD"; }

